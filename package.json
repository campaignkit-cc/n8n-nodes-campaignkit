{
  "name": "n8n-nodes-campaignkit",
  "version": "1.0.2",
  "description": "n8n community node for CampaignKit Email Validation",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "campaignkit",
    "email-validation",
    "email-verification"
  ],
  "license": "MIT",
  "homepage": "https://campaignkit.cc",
  "author": {
    "name": "CampaignKit",
    "email": "support@campaignkit.cc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/campaignkit-cc/n8n-nodes-campaignkit.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy:assets",
    "copy:assets": "copyfiles -u 1 \"nodes/**/*.{png,svg}\" \"credentials/**/*.{png,svg}\" dist/",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CampaignKitApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CampaignKit/CampaignKit.node.js",
      "dist/nodes/CampaignKit/CampaignKitTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "prettier": "^3.0.0",
    "typescript": "~5.3.0"
  },
  "dependencies": {
    "n8n-workflow": "*"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
